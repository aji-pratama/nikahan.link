[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=niklink
Group=www-data
WorkingDirectory=/home/niklink/niklink
ExecStart=/home/niklink/.pyenv/shims/gunicorn --access-logfile - --workers 3 --bind unix:/home/niklink/gunicorn/niklink.sock config.wsgi:application

[Install]
WantedBy=multi-user.target
